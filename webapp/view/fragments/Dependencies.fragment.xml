<core:FragmentDefinition height="100%" xmlns="sap.gantt.simple" xmlns:axistime="sap.gantt.axistime" xmlns:config="sap.gantt.config"
	xmlns:mvc="sap.ui.core.mvc" xmlns:shapes="sap.gantt.simple.shapes" xmlns:table="sap.ui.table" xmlns:m="sap.m" xmlns:core="sap.ui.core">
	<GanttChartContainer>
		<toolbar>
			<ContainerToolbar></ContainerToolbar>
		</toolbar>
		<GanttChartWithTable shapeSelectionMode="Single">
			<axisTimeStrategy>
				<axistime:ProportionZoomStrategy>
					<axistime:totalHorizon>
						<config:TimeHorizon startTime="20181029000000" endTime="20181129000000"/>
					</axistime:totalHorizon>
					<axistime:visibleHorizon>
						<config:TimeHorizon startTime="20181029000000" endTime="20181129000000"/>
					</axistime:visibleHorizon>
				</axistime:ProportionZoomStrategy>
			</axisTimeStrategy>
			<table>
				<!--<table:Table id="idTableDependencies" selectionMode="Single" visibleRowCountMode="Auto" selectionBehavior="RowSelector"
					enableColumnReordering="true" rows="{ path: '/root'}">
					<table:columns>
						<table:Column label="Col 1" template="text"/>
					</table:columns>
					<table:rowSettingsTemplate>
						<GanttRowSettings rowId="{id}" shapes1="{path: 'task', templateShareable:false}" shapes2="{path: 'subtask', templateShareable:false}">
							<shapes1>
								<shapes:Task time="{path: 'startTime', formatter: '.fnTimeConverter'}" endTime="{path: 'endTime', formatter: '.fnTimeConverter'}"
									type="SummaryExpanded" color="sapUiAccent5"/>
							</shapes1>
							<shapes2>
								<shapes:Task time="{path: 'startTime', formatter: '.fnTimeConverter'}" endTime="{path: 'endTime', formatter: '.fnTimeConverter'}"/>
							</shapes2>
						</GanttRowSettings>
					</table:rowSettingsTemplate>
				</table:Table>-->
				<table:Table selectionMode="Single" rows="{/WOHeaderSet}"  visibleRowCountMode="Auto" selectionBehavior="RowOnly">
					<table:columns>
						<table:Column sortProperty="ObjectKey">
							<m:Text text="Sort Id"/>
							<table:template>
								<m:Text text="{ObjectKey}" wrapping="false"/>
							</table:template>
						</table:Column>
						<table:Column sortProperty="ORDER_NUMBER">
							<m:Text text="Order Number"/>
							<table:template>
								<m:Text text="{ORDER_NUMBER}" wrapping="false"/>
							</table:template>
						</table:Column>
						<table:Column sortProperty="">
							<m:Text text="Operation Number"/>
							<table:template>
								<m:Text text="" wrapping="false"/>
							</table:template>
						</table:Column>
					</table:columns>
				</table:Table>
			</table>
		</GanttChartWithTable>
	</GanttChartContainer>
</core:FragmentDefinition>