
<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:smartFilterBar="sap.ui.comp.smartfilterbar">
	<Dialog id="idOrderFilterDialog" title="{tit.orderFilter}" draggable="true" class="sapUiTinyMargin" contentWidth="100%">
		<content>
			<smartFilterBar:SmartFilterBar id="idOrderFilterBar" entitySet="WOHeaderSet" useDateRangeType="true" 
				persistencyKey="com.evorait.evosuite.evomanagedepend.OrderFilterBar" initialized="onInitializedSmartVariant">
				<smartFilterBar:layoutData>
					<core:ExtensionPoint name="FilterExtP|OrderTableFilter|WOHeaderSet|LayoutData"/>
				</smartFilterBar:layoutData>
				<smartFilterBar:controlConfiguration>
					<core:ExtensionPoint name="FilterExtP|OrderTableFilter|WOHeaderSet|ControlConfiguration"/>
				</smartFilterBar:controlConfiguration>
				<smartFilterBar:groupConfiguration>
					<core:ExtensionPoint name="FilterExtP|OrderTableFilter|WOHeaderSet|GroupConfiguration"/>
				</smartFilterBar:groupConfiguration>
			</smartFilterBar:SmartFilterBar>
		</content>
		<buttons>
			<core:ExtensionPoint name="DialogExtP|OrderTableFilter|WOHeaderSet|Buttons"/>
			<Button text="{i18n>btn.close}" tooltip="{i18n>btn.close}" press="onOrderFilterClose"></Button>
		</buttons>
	</Dialog>
</core:FragmentDefinition>