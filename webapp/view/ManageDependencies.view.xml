<mvc:View controllerName="com.evorait.evosuite.evomanagedepend.controller.ManageDependencies" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m" xmlns:f="sap.f" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" height="100%">
	<Page id="idPageManageDepend" title="{i18n>title}" class="sapUiNoContentPadding" enableScrolling="false">
		<customHeader>
			<Bar>
				<contentLeft>
					<!--<Image src="{viewModel>/logoUrl}" densityAware="false" height="30px" alt="EvoTime"/>-->
				</contentLeft>
				<contentMiddle>
					<Title text="{i18n>title}"/>
				</contentMiddle>
				<contentRight>
					<Button id="idButtonAboutIcon" tooltip="About" icon="sap-icon://sys-help" press="onIconPress"/>
				</contentRight>
			</Bar>
		</customHeader>
		<content>
			<f:DynamicPage headerExpanded="true" preserveHeaderStateOnScroll="true" showFooter="false" class="sapUiNoContentPadding">
				<f:content>
					<l:ResponsiveSplitter defaultPane="default">
						<l:PaneContainer orientation="Vertical">
							<l:SplitPane>
								<core:Fragment fragmentName="com.evorait.evosuite.evomanagedepend.view.fragments.Dependencies" type="XML"/>
							</l:SplitPane>
							<l:SplitPane >
								<core:Fragment fragmentName="com.evorait.evosuite.evomanagedepend.view.fragments.OrderTable" type="XML"/>
							</l:SplitPane>
						</l:PaneContainer>
					</l:ResponsiveSplitter>
				</f:content>
			</f:DynamicPage>
		</content>
	</Page>
</mvc:View>